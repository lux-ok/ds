{
  "name": "@lux721/ds",
  "version": "0.1.1",
  "description": "Generic data set control library for js/ts",
  "author": "Thomas Choi",
  "license": "MIT",
  "keywords": [
    "javascript",
    "typescript",
    "npm",
    "generic dataset",
    "generic data tables",
    "tables",
    "rows"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomaschoi721/ds.git"
  },
  "scripts": {
    "build": "bunx rimraf dist && bunx tsc --project tsconfig.json",
    "clean": "bunx rimraf dist",
    "dev": "bunx tsc --watch",
    "test": "bun test",
    "prepublishOnly": "bun run clean && bun run build && echo '{\"type\": \"module\"}' > dist/package.json"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "exports": {
    "import": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
