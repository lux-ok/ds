{
  "name": "@lux721/ds",
  "version": "0.2.2",
  "author": "Thomas Choi",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomaschoi721/ds.git"
  },
  "description": "Generic dataset control library for js/ts",
  "keywords": [
    "javascript",
    "typescript",
    "npm",
    "generic dataset",
    "generic data tables",
    "multi selection",
    "crud FSM",
    "tables",
    "rows"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.MD"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./func": {
      "require": "./dist/func/index.cjs",
      "import": "./dist/func/index.js",
      "types": "./dist/func/index.d.ts"
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "better-docs": "^2.7.3",
    "jsdoc": "^4.0.4",
    "jsdoc-plugin-typescript": "^3.2.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.8",
    "rollup-plugin-delete": "^2.1.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.27.7",
    "typescript": "^5.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "doc": "jsdoc -c jsdoc.config.json",
    "doc-clean": "rimraf docs"
  }
}
