{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src/ds.ts"],
    "includePattern": ".*\\.ts$"
  },
  "plugins": ["jsdoc-plugin-typescript"],
  "opts": {
    "destination": "./docs",
    "template": "/home/admin/Projects/ds/node_modules/better-docs",
